FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant";
    object      electroMagnetics;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

solutionDomain		plasma;

poissonEquationSolver	semiImplicit;

emcModel		power;

spatioTemporal 		oneD;

powerCoeffs
{
	mode 					continuousFrequencyModulated;
	initialAmplitude		100;
	frequency 				13.56E6;
	bias					0;
	power 					5;
	dampingFactor			0.005;
	waveform				sinusoidal;
	operation				symmetricalBipolar;
	dutyCycle				0;
	naturalFrequency		0;
	dampingRatio			0;
	tolerance				1;			
}

oneDCoeffs
{
	frequency		13.56E6;
	outputInterval	1;
	nCycles			2;
	deltaT			5E-11;	
}


