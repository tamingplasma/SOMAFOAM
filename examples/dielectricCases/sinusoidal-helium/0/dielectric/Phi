FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      Phi;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [ 0 0 0 1 0 0 0 ];

internalField   uniform 0;

boundaryField
{
    plasma
    {
	type            plasmaDielectricWall;
	value		uniform 0;
	neighbourRegionName	region0;
	neighbourPatchName	dielectric;
	neighbourFieldName	Phi;
    }
    otherfaces
    {
        type            empty;
    }
    
    ground
    {
        type    fixedValue;
        value   uniform 0;
    }
    
    electrode
    {
        type plasmaExtendedPotential;
        // type            plasmaPotential;
        // value           uniform 0;
        // model           sinFrequencyModulated;
        // amplitude       uniform 600;
        // frequency   	13.56E6;
        // bias        	0.0;  
    }
    
}


